<p>&nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="footer-links"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="footer-item"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;i class="fas fa-lock"&gt;&lt;/i&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;span&gt;Forgot password&lt;/span&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="footer-item"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;i class="fas fa-headset"&gt;&lt;/i&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;span&gt;Customer Service&lt;/span&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&lt;div class="footer-item chat-icon"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;img src="https://www.flaticon.com/svg/v2/svg/3031/3031268.svg" alt="Chat Icon"&gt; &lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &lt;script&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; /* JavaScript (script.js) code starts here */<br>&nbsp; &nbsp; &nbsp; &nbsp; document.addEventListener('DOMContentLoaded', function() {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const phoneTab = document.getElementById('phone-tab');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const emailTab = document.getElementById('email-tab');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const phoneContent = document.getElementById('phone-content');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const emailContent = document.getElementById('email-content');</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; // Set initial active tab based on your preference.<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; // If you want phone number tab to be active by default, change 'emailTab' to 'phoneTab' below.<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; // For now, it's set to email login as per your image.<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailTab.classList.add('active');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneContent.style.display = 'none';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailContent.style.display = 'block';</p>
<p><br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneTab.addEventListener('click', function() {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneTab.classList.add('active');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailTab.classList.remove('active');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneContent.style.display = 'block';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailContent.style.display = 'none';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; });</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailTab.addEventListener('click', function() {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailTab.classList.add('active');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneTab.classList.remove('active');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; emailContent.style.display = 'block';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; phoneContent.style.display = 'none';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; });<br>&nbsp; &nbsp; &nbsp; &nbsp; });</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; function togglePasswordVisibility() { // For Email password field<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const passwordField = document.getElementById('password');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const toggleIcon = document.querySelector('#email-content .toggle-password i'); // Specific to email content</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if (passwordField.type === 'password') {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; passwordField.type = 'text';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.remove('fa-eye-slash');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.add('fa-eye');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; } else {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; passwordField.type = 'password';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.remove('fa-eye');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.add('fa-eye-slash');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; }<br>&nbsp; &nbsp; &nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; function togglePasswordVisibilityPhone() { // For Phone password field<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const passwordField = document.getElementById('phone-password');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; const toggleIcon = document.querySelector('#phone-content .toggle-password i'); // Specific to phone content</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if (passwordField.type === 'password') {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; passwordField.type = 'text';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.remove('fa-eye-slash');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.add('fa-eye');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; } else {<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; passwordField.type = 'password';<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.remove('fa-eye');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toggleIcon.classList.add('fa-eye-slash');<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; }<br>&nbsp; &nbsp; &nbsp; &nbsp; }<br>&nbsp; &nbsp; &nbsp; &nbsp; /* JavaScript (script.js) code ends here */<br>&nbsp; &nbsp; &lt;/script&gt;<br>&lt;/body&gt;<br>&lt;/html&gt;</p></body></html>